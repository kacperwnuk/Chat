<view name="active_sessions" layer="0" collapse-mode="2" max-obj-count="1">
	<schema name="public"/>
	<role name="postgres"/>
	<position x="700" y="540"/>
	<reference>
		<expression><![CDATA[ SELECT sessions.input_time,
    sessions.version,
    sessions.server_address,
    sessions.args,
    sessions.user_id,
    sessions.session_id
   FROM sessions
  WHERE (sessions.input_time > (CURRENT_TIMESTAMP - '00:02:00'::interval));]]></expression>
	<column name="input_time">
		<type name="timestamp with time zone" length="0" with-timezone="true"/>
	</column>
	<column name="version">
