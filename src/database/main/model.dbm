		execution-cost="1"
		row-amount="0">
	<schema name="public"/>
	<role name="postgres"/>
	<language name="plpgsql" sql-disabled="true"/>
	<return-type>
	<type name="trigger" length="0"/>
	</return-type>
	<definition><![CDATA[begin

insert into public.session_events
    ("version", session_id, "event", args)
values ('database', new.session_id, 'create','{}'::json);

return new;

end]]></definition>
</function>

